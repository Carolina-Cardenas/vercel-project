{
  "compilerOptions": {
    "incremental": true,
    "tsBuildInfoFile": "./.cache/tsconfig.node.tsbuildinfo",
    "target": "ES2022",
    "lib": ["ES2020"],
    "module": "CommonJS",
    "skipLibCheck": true,
    "moduleResolution": "Node",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true
  },
  "include": ["src"]
}

